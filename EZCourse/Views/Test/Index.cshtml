@model List<EZCourse.Controllers.TestController.User>
@{

    ViewData["Title"] = "Title of Test - Index";
    var countHtml = $"<h2>{Model.Count} users</h2>";
}

@Html.Raw(countHtml)
<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <th>@user.Id @(user.Id == 2 ? "*" : "")</th>
            <th>@user.Name</th>
        </tr>
    }
</table>


